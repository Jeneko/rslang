{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "airbnb-base",
    "airbnb-typescript/base"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "rules": {
    "no-console": 0,
    "max-len": 0,
    "no-param-reassign": 0,
    "@typescript-eslint/no-explicit-any": 2,
    "@typescript-eslint/no-use-before-define": 0
  }
}
